{
  "name": "caching",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/CommunityXYZ/caching.git",
  "author": "Cedrik <cedrik.git@tryninja.io>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && node -r dotenv/config build/server.js",
    "build": "esbuild src/server.ts --bundle --sourcemap --platform=node --outfile=build/server.js",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "dotenv": "^8.2.0",
    "esbuild": "^0.8.6",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "tedis": "^0.1.12"
  }
}
